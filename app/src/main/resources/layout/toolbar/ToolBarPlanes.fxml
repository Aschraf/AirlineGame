<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="80.0" prefWidth="80.0" styleClass="tool-box" xmlns="http://javafx.com/javafx/15.0.1">
    <Label alignment="CENTER" maxWidth="1.7976931348623157E308" styleClass="tool-box-main-label" text="Planes" VBox.vgrow="ALWAYS"/>
    <GridPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" VBox.vgrow="ALWAYS">
        <columnConstraints>
            <ColumnConstraints hgrow="NEVER" maxWidth="1.7976931348623157E308" minWidth="10.0"/>
            <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" minWidth="10.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <ImageView fx:id="planesImageView" fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true"/>
        <ImageView fx:id="maintenanceView" fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true" GridPane.rowIndex="1"/>
        <Label fx:id="planesLabel" styleClass="tool-box-info-label" text="Label" GridPane.columnIndex="1"/>
        <Label fx:id="maintenanceLabel" styleClass="tool-box-info-label" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    </GridPane>
</VBox>
